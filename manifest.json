{
  "manifest_version": 2,
  "name": "bell.lahs.club Extension",
  "description": "An extension that displays the time remaining in classes at Los Altos High School, as seen on the website bell.lahs.club.",
  "version": "1.0.2",
  "content_security_policy": "script-src 'self' https://bell.lahs.club; object-src 'self'",
  "browser_action": {
    "default_icon": "favicons/lime.png",
    "default_popup": "chrome_extension/popup.html",
    "default_title": "Bell"
  },
  "permissions": [
    "cookies",
    "alarms",
    "https://bell.lahs.club/*",
    "http://bell.lahs.club/*"
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}